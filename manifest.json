{
    "manifest_version": 3,
    "name": "ChatGPT Chrome Extension",
    "version": "1.0",
    "description": "Send prompts to ChatGPT and display responses.",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
      }
    },
    "icons": {
      "16": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    },
    "background": {
        "service_worker": "background.js"
    }
  }
  